{
  "name": "env-protector",
  "displayName": "env-protector",
  "description": "Protect your secrets from being exposed",
  "version": "1.0.9",
  "engines": {
    "vscode": "^1.103.0"
  },
  "publisher": "sametcn99",
  "icon": "icon.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    ".env",
    "cloak",
    "development",
    "dotenv-vault",
    "dotenv",
    "env",
    "environment manager",
    "environment variables",
    "environment",
    "file",
    "hide",
    "mask",
    "productivity",
    "protect",
    "secrets",
    "security"
  ],
  "homepage": "https://sametcc.me/env-protector",
  "author": {
    "email": "sametcn99@gmail.com",
    "name": "sametcn99",
    "url": "https://sametcc.me"
  },
  "license": "GPL-3.0-only",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sametcn99/env-protector"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "env-protector.hide-env-files",
        "title": "Hide Environment Files",
        "shortTitle": "Hide env files",
        "category": "env-protector"
      },
      {
        "command": "env-protector.show-env-files",
        "title": "Show Environment Files",
        "category": "env-protector"
      },
      {
        "command": "env-protector.mask-env-values",
        "title": "Mask Environment Values",
        "category": "env-protector"
      },
      {
        "command": "env-protector.add-env-variable",
        "title": "Add Environment Variable",
        "category": "env-protector"
      },
      {
        "command": "env-protector.edit-env-variable",
        "title": "Edit Environment Variable",
        "category": "env-protector"
      },
      {
        "command": "env-protector.remove-env-variable",
        "title": "Remove Environment Variable",
        "category": "env-protector"
      }
    ],
    "customEditors": [
      {
        "viewType": "env-protector.envFileEditor",
        "displayName": "env-protector Environment File Editor",
        "priority": "default",
        "selector": [
          {
            "filenamePattern": "**/.env*"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "prettier": "npx prettier . --write",
    "vsce": "vsce package"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "24.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.35.0",
    "node-loader": "^2.1.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "bufferutil": "^4.0.9",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "utf-8-validate": "^6.0.5"
  }
}
